+/**
+ * ðŸ”’ WebSocket & Cache Types
+ * 
+ * Type-safe notification payloads and cached entities.
+ * Eliminates 'any' types in WebSocket handlers and Redis cache.
+ * 
+ * @module types/notifications
+ */
+
+/**
+ * WebSocket notification payload data
+ */
+export interface NotificationPayload {
+  connectionId?: string;
+  features?: string[];
+  userId?: string;
+  interests?: string[];
+  preferences?: {
+    sound?: boolean;
+    vibration?: boolean;
+    desktop?: boolean;
+  };
+  [key: string]: unknown;  // Allow additional fields
+}
+
+/**
+ * WebSocket subscription message body
+ */
+export interface SubscriptionData {
+  userId?: string;
+  interests?: string[];
+}
+
+/**
+ * User entity stored in Redis cache
+ */
+export interface CachedUser {
+  id: string;
+  email: string;
+  name: string;
+  level: number;
+  preferences?: Record<string, unknown>;
+}
+
+/**
+ * Redis client error with additional properties
+ */
+export interface RedisError extends Error {
+  code?: string;
+  errno?: string | number;
+  syscall?: string;
+}
